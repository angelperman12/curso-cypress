{
  "name": "curso-cypress",
  "version": "1.0.0",
  "description": "Curso de automatizaci√≥n de pruebas con cypress",
  "main": "index.js",
  "scripts": {
    "limpiar-reporte": "rimraf ./allure-report  && rimraf ./allure-results",
    "ejecutar-reporte": "cypress run --headed --env allure=true",
    "generar-reporte": "allure generate allure-results -o allure-report --clean",
    "abrir-reporte": "allure open",
    "test": "npm run limpiar-reporte && npm run ejecutar-reporte; npm run generar-reporte && npm run abrir-reporte"
    
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/angelperman12/curso-cypress.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/angelperman12/curso-cypress/issues"
  },
  "homepage": "https://github.com/angelperman12/curso-cypress#readme",
  "devDependencies": {
    "cypress": "^13.13.0"
  },
  "dependencies": {
    "@mmisty/cypress-allure-adapter": "^2.5.0",
    "@shelex/cypress-allure-plugin": "^2.40.2",
    "allure-commandline": "^2.29.0"
  }
}
